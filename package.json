{
  "name": "drink-local",
  "version": "1.0.0",
  "description": "Wet Your Whistle: Local Brewery & Brewpub Locator",
  "main": "index.js",
  "scripts": {
    "dev": "snowpack dev",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "check": "svelte-check --diagnostic-sources \"js,svelte\"",
    "check:watch": "svelte-check --watch --diagnostic-sources \"js,svelte\"",
    "build": "npm run check && snowpack build",
    "start": "sirv build --maxage 36000"
  },
  "author": "Jim Strother",
  "license": "ISC",
  "devDependencies": {
    "@mdi/font": "^5.9.55",
    "@snowpack/plugin-dotenv": "^2.0.5",
    "@snowpack/plugin-svelte": "^3.5.2",
    "@snowpack/plugin-webpack": "^2.3.0",
    "autoprefixer": "^10.2.3",
    "babel-jest": "^26.6.3",
    "dotenv": "^8.2.0",
    "jest": "^26.6.3",
    "postcss": "^8.2.4",
    "snowpack": "^3.0.11",
    "svelte": "^3.31.2",
    "svelte-check": "^1.1.28",
    "svelte-jester": "^1.3.0",
    "svelte-preprocess": "^4.6.1"
  },
  "dependencies": {
    "sirv-cli": "^1.0.10",
    "tailwindcss": "^2.0.2"
  },
  "engines": {
    "node": "14.7.0",
    "npm": "6.14.7"
  }
}
